{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "server/*"
  ],
  "scripts": {
    "start:express": "lerna run start --stream --scope=@switch-stats/express.io --bail",
    "start:feathers": "lerna exec --scope @switch-stats/feathers -- yarn start --color --bail",
    "start:web": "lerna run start --stream --scope=@switch-stats/webroot --bail",
    "start": "yarn start:web & yarn start:feathers",
    "clean": "lerna run clean --stream",
    "distclean": "rimraf ./dist ./node_modules",
    "cleanall": "yarn distclean & lerna run cleanall --stream"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "lerna": "^3.4.3",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-bundle-manifest": "^0.2.0",
    "parcel-plugin-static-files-copy": "^1.2.4",
    "rimraf": "^2.6.2",
  },
  "dependencies": {
    "config": "^3.0.0",
    "debug": "^4.1.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2"
  }
}
